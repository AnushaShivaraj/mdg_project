<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog id="myDialog13" contentWidth="30%" noDataText="No Customers Found" title="Select Customers" search="handleSearchCustomerPress"
		confirm="onConfirmCustomer" cancel="handleClose" multiSelect="true" items="{ path : '/' }">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier title="{attributes/customerName}" text="{attributes/contact}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column >
				<header>
					<Text text="{i18n>name}"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>