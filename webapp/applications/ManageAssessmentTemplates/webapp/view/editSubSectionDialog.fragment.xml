<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog title="{i18nModel>EditSection}">
		<f:SimpleForm >
			<Label text="{i18nModel>SectionName}"/>
			<Input  width="100%"></Input>
		</f:SimpleForm>
		<beginButton>
			<Button text="{i18nModel>Save}" press="onEditSectionSavePress" type="Emphasized" tooltip="{i18nModel>Save}"/>
		</beginButton>
		<endButton>
			<Button text="{i18nModel>Cancel}" press="onEditDialogCancelPress" tooltip="{i18nModel>Cancel}"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>