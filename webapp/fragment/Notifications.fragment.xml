<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
	class="sapUiBodyBackground sapContrastPlus">
	<Popover title="{i18n>Notifications}" placement="Bottom" contentWidth="20rem"
		class="sapUiResponsivePadding--header sapUiResponsivePadding--footer">
		<VBox class="sapUiSmallMargin">
			<NotificationList items="{/}">
				<layoutData>
					<FlexItemData maxWidth="600px"/>
				</layoutData>
				<NotificationListItem title="{attributes/name}" description="{attributes/description}" showCloseButton="true"
					datetime="{attributes/endDate}" unread="true" priority="Medium" close=".onItemClose" authorName="{attributes/priority}"
					authorInitials="{attributes/status}"></NotificationListItem>
			</NotificationList>
		</VBox>
	</Popover>
</core:FragmentDefinition>
